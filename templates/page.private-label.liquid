{% comment %}
  To edit the contents of this file, look inside the `sections` folder for the file name that comes directly after `section`. Eg. {% section 'contact__main' %} or {% section 'contact__banner' %}
{% endcomment %}

<main id="template-{{ template | replace: '.', '-' | handle }}" class="shopify-section private-label-page" data-check-for-order="true">
  <section class="section__wrapper">
    {% section 'page-label__sections' %}
    {% section 'page-label__main' %}
  </section>
</main>
